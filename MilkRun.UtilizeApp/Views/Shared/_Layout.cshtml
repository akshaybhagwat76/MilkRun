<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] MilkRun</title>

    <link href="@Url.Content("~/lib/plugins/fontawesome-free/css/all.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/lib/dist/css/adminlte.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/lib/dist/css/custom-style.css")" rel="stylesheet" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
</head>
<body>
    @Html.Partial("_Navbar")

    <div class="hold-transition login-page login-page-body">
        @RenderBody()
    </div>

    <script src="@Url.Content("~/lib/plugins/bootstrap/js/bootstrap.bundle.min.js")"></script>
    <script src="@Url.Content("~/lib/dist/js/adminlte.min.js")"></script>
    <script src="@Url.Content("~/lib/bootstrap/dist/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/lib/plugins/bootstrap/js/bootstrap.bundle.min.js")" asp-append-version="true"></script>
    <script src="@Url.Content("~/lib/dist/js/adminlte.min.js")" asp-append-version="true"></script>

    @await Html.PartialAsync("_ValidationScriptsPartial")

    @RenderSection("Scripts", required: false)

</body>
</html>
